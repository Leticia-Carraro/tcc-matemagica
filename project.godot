config_version=5

[application]

config/name="Mathistorys"
run/main_scene="res://welcome/welcome_screen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")

[audio]

buses/default_bus_layout="res://assets/audio/default_bus_layout.tres"
general/text_to_speech=true

[autoload]

AudioController="*res://assets/audio/audio_controller.tscn"
AutoloadScene="*res://scenes/game_objects/AutoloadScene.gd"
Global="*res://config.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"character": "res://assets/Char/character.dch",
"narr": "res://assets/Char/narr.dch"
}
directories/dtl_directory={
"Quiz": "res://assets/Timeline/Quiz.dtl",
"intro_balanca": "res://balanca_jogo/intro_balanca.dtl",
"rg_ans": "res://assets/Timeline/rg_ans.dtl",
"wrong_ans": "res://assets/Timeline/wrong_ans.dtl",
"wrong_simple": "res://assets/Timeline/wrong_simple.dtl"
}
